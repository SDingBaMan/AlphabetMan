<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.sdingba.su.alphabet_demotest.view.newsActivity">

    <RelativeLayout
        android:id="@+id/title"
        android:layout_width="fill_parent"
        android:layout_height="60dp"

        android:background="#111211">

        <TextView
            android:id="@+id/maxall_back"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_centerInParent="true"
            android:background="@drawable/selector_head_back"
            android:clickable="true"
            android:gravity="center"
            android:text="返 回"
            android:textColor="#ffebebeb"
            android:textSize="15sp"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/id_main_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerInParent="true"
            android:text="分析数据"
            android:textColor="#ffffff"
            android:textSize="20sp"
            android:textStyle="bold" />


    </RelativeLayout>

    <LinearLayout

        android:id="@+id/news_view_tile"
        android:layout_width="match_parent"
        android:gravity="center"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <TextView
            android:id="@+id/allmax"
            android:gravity="center"
            android:textSize="30dip"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="暂无数据..." />

    </LinearLayout>


</LinearLayout>
